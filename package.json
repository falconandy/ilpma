{
  "name": "@falconandy/ilpma",
  "description": "Ilpma CLI",
  "version": "0.1.23",
  "author": "falconandy",
  "bin": {
    "ilpma": "./bin/run"
  },
  "dependencies": {
    "@babel/parser": "^7.12.11",
    "@babel/traverse": "^7.12.12",
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "^3.8.3",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@phenomnomnominal/tsquery": "^3.0.0",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/jest": "^24.0.23",
    "@types/node": "^10",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^25.3.0",
    "jest-mock-process": "^1.3.2",
    "jest-serial-runner": "^1.1.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "ts-node": "^8",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "ilpma",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-warn-if-update-available",
      "@oclif/plugin-autocomplete"
    ],
    "update": {
      "node": {
        "version": "12.16.3",
        "targets": []
      }
    }
  },
  "scripts": {
    "clean": "rimraf tsconfig.tsbuildinfo \"lib/*\" \"dist/*\"",
    "prepack": "yarn build && oclif-dev manifest && oclif-dev readme",
    "postpack": "rimraf oclif.manifest.json",
    "lint": "eslint --ext .ts ./src ./test",
    "build": "yarn clean && tsc -b",
    "test": "APP_ENV='development' jest --runInBand --colors",
    "test-ci": "APP_ENV='development' jest --runInBand --colors",
    "test-ci-v": "TEST_VERBOSE=true API_URL='https://api-dev.iterative.ly/graphql' APP_ENV='development' jest --runInBand --colors",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
